{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todo",
  "type": "object",
  "properties": {
    "dueDate": {
      "type": "string",
      "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
    },
    "name": {
      "type": "string"
    }
  },
  "required": [
    "dueDate",
    "name"
  ],
  "additionalProperties": false
}
